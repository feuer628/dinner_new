{
  "name": "dinner-new",
  "description": "A VueJS + NodeJS project",
  "version": "1.0.1",
  "author": "Cheremnov",
  "private": true,
  "scripts": {
    "dev"         : "npm run back_prod && npm-run-all --parallel front:watch back_dev",
    "front_dev"   : "npm-run-all --parallel front:**",
    "front:watch" : "webpack -w --config frontend/webpack.dev.js",
    "front:server": "webpack-dev-server --config frontend/webpack.dev.js",
    "back_dev"    : "npm-run-all --parallel back:**",
    "back:watch"  : "webpack -w --config backend/webpack.config.js --mode=development",
    "back:server" : "nodemon ./build/app.js localhost 80",
    "prod"        : "npm-run-all front_prod back_prod",
    "front_prod"  : "webpack --config frontend/webpack.prod.js",
    "back_prod"   : "webpack --config backend/webpack.config.js --mode=production"
  },
  "dependencies": {
    "axios": "latest",
    "body-parser": "1.19.0",
    "bootstrap-vue": "^2.0.0-rc.25",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "~4.17.1",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "node-sass-middleware": "^0.11.0",
    "node-schedule": "^1.3.2",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.9.4",
    "sequelize-typescript": "^0.6.11",
    "simple-node-logger": "^18.12.22",
    "typescript": "^3.5.3",
    "vue": "^2.6.10",
    "vue-router": "^3.0.7",
    "vue-class-component": "^7.1.0",
    "vue-meta": "^2.0.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/node-sass-middleware": "0.0.31",
    "@types/node-schedule": "^1.2.3",
    "@types/vue": "^2.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "dts-gen": "^0.5.8",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.4",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
